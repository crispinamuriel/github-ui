{{#if this.model.length}}
<ul>
    {{#each this.model as |issue|}}
      <div class="issue">
        <li class="title">Issue #{{issue.number}} - {{issue.title}}</li>
        <p>{{issue.body}}</p>
      </div>

      {{!-- <li># 123  - Issue description goes here</li> --}}
    {{/each}}
</ul>
{{else}}
<h3>There are no current issues for this repository.</h3>
{{/if}}
{{outlet}}
